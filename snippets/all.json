{
  "todo": {
    "prefix": "todo",
    "body": "TODO: $1"
  },
  "fixme": {
    "prefix": "fixme", 
    "body": "FIXME: $1"
  },
  "hack": {
    "prefix": "hack",
    "body": "HACK: $1"
  },
  "note": {
    "prefix": "note",
    "body": "NOTE: $1"
  },
  "warning": {
    "prefix": "warning",
    "body": "WARNING: $1"
  },
  "timestamp": {
    "prefix": "timestamp",
    "body": "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE} ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}"
  },
  "date": {
    "prefix": "date",
    "body": "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}"
  },
  "time": {
    "prefix": "time",
    "body": "${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}"
  },
  "header": {
    "prefix": "header",
    "body": [
      "/******************************************************************************",
      " * @file        $1",
      " * @brief       $2",
      " * @author      $3",
      " * @email       $4",
      " * @date        $5",
      " * @version     1.0.0",
      " * @license     $6",
      " * @copyright   Copyright (c) $7",
      " ******************************************************************************/",
      ""
    ]
  }
}