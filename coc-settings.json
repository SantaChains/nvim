  "outline.autoCloseOnJump": true,
  "outline.title": "Outline",
  "outline.markdownAsTxt": false,
  "outline.keepWindow": false,
  "list.previewHighlightGroup": "Statement",
  "list.nextKeymap": "<C-j>",
  "list.previousKeymap": "<C-k>",
